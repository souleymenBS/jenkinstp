pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build'){
            steps{
                echo'dev'
            }
        }
        stage('test'){
            steps{
                echo'testing'
            }
        }
        stage('deploy'){
            steps{
                echo'mise en production'
            }
        }
    }
}
